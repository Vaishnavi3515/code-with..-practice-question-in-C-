#include <bits/stdc++.h>
using namespace std;
int main(){
   int m;
   cout<<"enter the marks(out of 100):"<<endl;
   cin>>m;
   if(m>90 && m<=100)
   cout<<"grade is A+";
   else if(m>80 && m<=90)
   cout<<"grade is A";
   else if(m>70 && m<=80)
   cout<<"grade is B";
   else if(m>60 && m<=70)
   cout<<"grade is C";
   else if(m>50 && m<=60)
   cout<<"grade is D";
   else 
   cout<<"invalid score"<<endl;
            return 0;
        }
