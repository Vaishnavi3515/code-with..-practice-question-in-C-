#include <bits/stdc++.h>
#include<string>
using namespace std;
int main() {
    string s;
    cin>>s;
    int flag = 1;
    for(int i=0;i<s.length()/2;i++){
        if(s[i]!=s[s.length()-1-i]){
            flag =0;
            break;
        }}
        if(flag)
        cout<<"palindrome";
        else
        cout<<"not palindrome";
        return 0;
}
