#include <bits/stdc++.h>
#include<string>
using namespace std;
int main (){
    string s1,s2,s3;
    cout<<"enter the string1:"<<endl;
    getline(cin,s1);
    cout<<"enter the string2:"<<endl;
    getline(cin,s2);
    s3 = s1+s2;
    cout<<"concentrate strings:"<<s3<<endl;
    cout<<"length of string:"<<s3.length()<<endl;
    return 0;
}
