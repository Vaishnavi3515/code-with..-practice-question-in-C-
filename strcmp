#include <bits/stdc++.h>
#include<cctype>
using namespace std;
int main() {
      char str1[]="banana";
      char str2[]="apple";
      int result= strcmp(str1,str2);
      if(result==0)
      cout<<"both the strings are equal."<<endl;
      else if (result<0)
      cout<<"str1 is less than str2."<<endl;
      else
      cout<<"str1 is greater than str2."<<endl;
    return 0;
}
